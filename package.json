{
  "name": "confidential-portfolio-manager",
  "version": "1.0.0",
  "type": "module",
  "description": "Confidential Portfolio Manager - Privacy Asset Management with FHE",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "node": "npx hardhat node",
    "deploy": "npx hardhat run scripts/deploy.js",
    "deploy-sepolia": "npx hardhat run scripts/deploy-sepolia.js --network sepolia",
    "deploy-portfolio": "npx hardhat run scripts/deploy-portfolio.cjs --network sepolia",
    "deploy-local": "npx hardhat run scripts/deploy-local.js --network localhost",
    "setup-local": "npx hardhat run scripts/setup-local.js --network localhost",
    "frontend": "cd frontend && npm start",
    "frontend-build": "cd frontend && npm run build:vercel",
    "frontend-install": "cd frontend && npm install",
    "start-3023": "cd frontend && DANGEROUSLY_DISABLE_HOST_CHECK=true SKIP_PREFLIGHT_CHECK=true PORT=3023 npm start",
    "verify": "npx hardhat run scripts/verify-contracts.js",
    "gas-report": "REPORT_GAS=true npx hardhat test",
    "coverage": "npx hardhat coverage",
    "clean": "npx hardhat clean && rm -rf frontend/build",
    "build": "npm run compile && npm run frontend-build",
    "vercel-build": "cd frontend && npm install && npm run build:vercel",
    "vercel-dev": "cd frontend && npm start"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "portfolio",
    "assets",
    "fhe",
    "privacy",
    "zama",
    "hardhat",
    "react",
    "confidential"
  ],
  "author": "Confidential Portfolio Team",
  "license": "UNLICENSED",
  "devDependencies": {
    "@fhevm/hardhat-plugin": "0.0.1-3",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.9",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.14",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "hardhat": "^2.22.0",
    "hardhat-deploy": "^0.11.45",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@fhevm/solidity": "^0.7.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.1.0",
    "ethers": "^6.4.0",
    "dotenv": "^16.0.0"
  }
}