{
  "name": "confidential-portfolio",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "compile": "cross-env hardhat compile",
    "test": "cross-env hardhat test",
    "deploy:sepolia": "cross-env hardhat run scripts/deploy.ts --network sepolia",
    "deploy:local": "cross-env hardhat run scripts/deploy.ts --network localhost",
    "verify-contract": "cross-env hardhat run scripts/verify-contract.ts --network sepolia",
    "node": "cross-env hardhat node"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "ethers": "6.15.0"
  },
  "devDependencies": {
    "@types/react": "18.3.24",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "4.7.0",
    "eslint": "8.57.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.20",
    "typescript": "5.9.2",
    "vite": "5.4.20",
    "@fhevm/solidity": "0.7.0",
    "@fhevm/hardhat-plugin": "0.0.1-3",
    "hardhat": "2.26.3",
    "@nomicfoundation/hardhat-toolbox": "4.0.0",
    "@nomicfoundation/hardhat-ethers": "3.1.0",
    "@types/node": "20.19.13",
    "cross-env": "7.0.3",
    "dotenv": "16.6.1"
  }
}